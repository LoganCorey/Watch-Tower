{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ]
    },
    "rulesDirectory": [],
    "max-line-length": [
        true,
        {
          "limit": 120,
          "ignore-pattern": "^import |^export {(.*?)}",
          "check-strings": true,
          "check-regex": true
        }
      ]
}